<!-- pgdoc-cn_start sig_en=63f8c47632249e9ccb47bc7e0695b747 sig_cn_org=None source=14.1 
  <indexterm zone="catalog-pg-opfamily">
   <primary>pg_opfamily</primary>
  </indexterm>
